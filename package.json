{
	"name": "website",
	"version": "1.0.0",
	"description": "Personal website of Germán Freixinós López, front-end web developer.",
	"keywords": [
		"11ty-website",
		"blog",
		"eleventy-site",
		"eleventy-website",
		"eleventy",
		"frontend-developer",
		"frontend-web-developer",
		"frontend-web",
		"frontend",
		"jamstack-site",
		"jamstack",
		"netlify",
		"personal-website",
		"portfolio-page",
		"portfolio-site",
		"portfolio-website",
		"portfolio",
		"potfolioweb",
		"static-site",
		"website"
	],
	"homepage": "https://github.com/germanfrelo/website#readme",
	"bugs": {
		"url": "https://github.com/germanfrelo/website/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/germanfrelo/website.git"
	},
	"license": "ISC",
	"author": "Germán Freixinós López",
	"main": ".eleventy.js",
	"scripts": {
		"build": "eleventy",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint:css": "stylelint --formatter verbose --max-warnings 0 \"**/*.css\"",
		"lint:js": "eslint --max-warnings 0 .",
		"prepare": "husky install",
		"sort-package-json": "sort-package-json",
		"start": "eleventy --serve",
		"stylelint-check": "stylelint-config-prettier-check"
	},
	"lint-staged": {
		"*": "prettier --write",
		"*.css": "stylelint",
		"*.js": "eslint",
		"package.json": "sort-package-json"
	},
	"devDependencies": {
		"@11ty/eleventy": "0.12.1",
		"@prettier/plugin-xml": "2.2.0",
		"eslint": "8.38.0",
		"eslint-config-prettier": "8.8.0",
		"husky": "8.0.3",
		"lint-staged": "13.2.1",
		"prettier": "2.8.7",
		"prettier-plugin-multiline-arrays": "1.1.3",
		"sort-package-json": "2.4.1",
		"stylelint": "14.16.1",
		"stylelint-config-prettier": "9.0.5",
		"stylelint-config-recess-order": "3.1.0",
		"stylelint-config-standard": "29.0.0"
	}
}
