{
	"quoteProps": "consistent",
	"overrides": [
		{
			"files": ["*.css", "*.html"],
			"options": {
				"printWidth": 9999
			}
		},
		{
			"files": ["*.js", "*.jsx", "*.vue"],
			"options": {
				"singleAttributePerLine": true
			}
		},
		{
			"files": ["*.jsonc"],
			"options": {
				"trailingComma": "none"
			}
		}
	]
}
